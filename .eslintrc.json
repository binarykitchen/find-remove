{
  "parser": "@babel/eslint-parser",
  "plugins": [],
  "extends": [
    "eslint:recommended",
    "standard",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "root": true,
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-else-return": 1,
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "ignore",
        "named": "ignore",
        "asyncArrow": "ignore"
      }
    ]
  },
  "globals": {}
}
