{
  "name": "find-remove",
  "version": "4.0.1",
  "description": "recursively finds files and/or directories by filter options from a start directory onwards and deletes these according to plenty of options you can configure. useful if you want to clean up stuff within a directory in your node.js app.",
  "main": "dist/find-remove.js",
  "author": "Michael Heuberger <michael.heuberger@binarykitchen.com>",
  "keywords": [
    "file",
    "remove",
    "rmdir",
    "unlink",
    "find",
    "filter",
    "recursive",
    "fs",
    "delete",
    "extension",
    "file stats"
  ],
  "engines": {
    "node": "^16.1.0",
    "npm": "^7.20.0"
  },
  "dependencies": {
    "fmerge": "1.2.0",
    "rimraf": "3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.8",
    "@babel/eslint-parser": "7.15.8",
    "@babel/node": "7.15.8",
    "@babel/preset-env": "7.15.8",
    "eslint": "7.25.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "microbundle": "0.14.1",
    "mkdirp": "1.0.4",
    "nodeunit": "0.11.3",
    "prettier": "2.4.1",
    "randomstring": "1.2.1"
  },
  "source": "src/index.js",
  "module": "dist/find-remove.mjs",
  "unpkg": "dist/find-remove.umd.js",
  "scripts": {
    "test": "nodeunit tests/basics.js",
    "lint": "eslint ./src ./tests",
    "lint:fix": "yarn --silent lint --fix; exit 0",
    "prettier": "prettier --check ./src ./tests",
    "prettier:fix": "prettier --write ./src ./tests",
    "build": "microbundle"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/binarykitchen/find-remove.git"
  },
  "license": "MIT",
  "readmeFilename": "README.md",
  "directories": {
    "test": "tests"
  }
}
